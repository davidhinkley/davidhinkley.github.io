{
  "name": "galleria-server",
  "version": "1.0.0",
  "description": "Backend for the Galleria application",
  "main": "server.js",
  "scripts": {
    "prestart": "node ensure-db-state.js",
    "start": "node server.js",
    "predev": "node ensure-db-state.js",
    "dev": "nodemon server.js",
    "setup": "node run-setup.js",
    "backup": "node backup-cli.js",
    "setup-backup": "bash setup-backup.sh",
    "install-backup-deps": "bash install-backup-deps.sh",
    "setup-scheduled-backups": "bash setup-scheduled-backups.sh",
    "run-scheduled-backup": "node scheduled-backup.js",
    "setup-complete-backup": "bash setup-complete-backup-system.sh",
    "add-sample-images": "node create-sample-images.js",
    "kill-server": "node kill-server.js",
    "dev-clean": "node kill-server.js && node ensure-db-state.js && nodemon server.js",
    "fix-persistence": "node fix-persistence.js",
    "check-db": "node check-db.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "inquirer": "^8.2.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "bin": {
    "galleria-backup": "./backup-cli.js"
  }
}